{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "areas",
    "game_indices",
    "id",
    "name",
    "names",
    "region"
  ],
  "properties": {
    "areas": {
      "$id": "#/properties/areas",
      "type": "array",
      "title": "The Areas Schema"
    },
    "game_indices": {
      "$id": "#/properties/game_indices",
      "type": "array",
      "title": "The Game_indices Schema",
      "items": {
        "$id": "#/properties/game_indices/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "game_index",
          "generation"
        ],
        "properties": {
          "game_index": {
            "$id": "#/properties/game_indices/items/properties/game_index",
            "type": "string",
            "title": "The Game_index Schema",
            "default": "",
            "examples": [
              "number"
            ],
            "pattern": "^(.*)$"
          },
          "generation": {
            "$id": "#/properties/game_indices/items/properties/generation",
            "type": "object",
            "title": "The Generation Schema",
            "required": [
              "name",
              "url"
            ],
            "properties": {
              "name": {
                "$id": "#/properties/game_indices/items/properties/generation/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "string"
                ],
                "pattern": "^(.*)$"
              },
              "url": {
                "$id": "#/properties/game_indices/items/properties/generation/properties/url",
                "type": "string",
                "title": "The Url Schema",
                "default": "",
                "examples": [
                  "string"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    },
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "number"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "names": {
      "$id": "#/properties/names",
      "type": "array",
      "title": "The Names Schema",
      "items": {
        "$id": "#/properties/names/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "language",
          "name"
        ],
        "properties": {
          "language": {
            "$id": "#/properties/names/items/properties/language",
            "type": "object",
            "title": "The Language Schema",
            "required": [
              "name",
              "url"
            ],
            "properties": {
              "name": {
                "$id": "#/properties/names/items/properties/language/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "string"
                ],
                "pattern": "^(.*)$"
              },
              "url": {
                "$id": "#/properties/names/items/properties/language/properties/url",
                "type": "string",
                "title": "The Url Schema",
                "default": "",
                "examples": [
                  "string"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "name": {
            "$id": "#/properties/names/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "string"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "region": {
      "$id": "#/properties/region",
      "type": "null",
      "title": "The Region Schema",
      "default": null,
      "examples": [
        null
      ]
    }
  }
}